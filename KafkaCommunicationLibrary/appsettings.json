{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Kafka": {
    "BootstrapServers": "localhost:9092",
    "Consumer": {
      "GroupId": "innkkkt-authentication-group",
      "AutoOffsetReset": "earliest",
      "EnableAutoCommit": true
    },
    "Producer": {
      "Acks": "Leader",
      "RetriesBackoffMs": 100,
      "MaxInFlight": 5
    }
  }
}
